{
	"name": "resterland/rlkickstart",
	"type": "typo3-cms-extension",
	"description": "Setting up a basic page tree for starting new web projects, multilingual. Default language is set to German. Additional languages = en, id, fr",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"TYPO3 CMS"
	],
	"authors": [
		{
			"name": "Roland Fuhrer",
			"email": "roland@resterland.ch",
			"homepage": "https://resterland.ch",
			"role": "Developer"
		}
	],
	"require": {
		"typo3/minimal": "^10 || ^11",
		"typo3/cms-core": "^10 || ^11",
		"typo3/cms-impexp": "^10 || ^11"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"friendsofphp/php-cs-fixer": "^2",
		"typo3/testing-framework": "^6"
	},
	"extra": {
		"typo3/cms": {
			"root-dir": "private",
			"web-dir": "public",
			"extension-key": "rlkickstart"
		}
	},
	"autoload": {
		"psr-4": {
			"Resterland\\Rlkickstart\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Resterland\\Rlkickstart\\Tests\\": "Tests"
		}
	},
	"config": {
		"optimize-autoloader": true,
		"sort-packages": true,
		"allow-plugins": {
			"typo3/cms-*": true,
			"resterland/*": true,
			"roave/security-advisories": true,
			"typo3/testing-framework": true,
			"helhum/*": true,
			"causal/image_autoresize": true,
			"typo3/class-alias-loader": true,
			"friendsofphp/php-cs-fixer": true
		}
	}
}
